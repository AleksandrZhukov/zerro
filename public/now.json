{
  "version": 2,
  "routes": [
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "^/icons/(.*)",
      "dest": "/icons/$1"
    },
    {
      "src": "^/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "^/social-preview.png",
      "dest": "/social-preview.png"
    },
    {
      "src": "^/service-worker.js",
      "dest": "/service-worker.js"
    },
    {
      "src": "^/precache-manifest(.*).js",
      "dest": "/precache-manifest$1.js"
    },
    {
      "src": "^/(.*)",
      "dest": "/index.html"
    }
  ],
  "public": false
}
